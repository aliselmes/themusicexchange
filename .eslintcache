       search: ''\n        }\n    }\n\n    updateSearch(event) {\n        this.setState({search: event.target.value});\n    }\n\n    render() {\n\n        let filteredItems = this.props.items.filter(\n            (item) => {\n                return item.location.toLowerCase().indexOf(this.state.search.toLowerCase()) !==-1 ;\n            }\n        ) ;\n\n        return (\n            <div className=\"container my-5\">\n                <div className=\"row\">\n                    <div className=\"col\">\n                        <h2>Browse Items</h2>\n                        <hr />\n                    </div>\n                </div>\n                <div className=\"row mb-3\">\n                    <div className=\"col-md-4\">\n                        <Form>\n                            <FormGroup>\n                                <Label htmlFor=\"location\">Search your community:</Label>\n                                <Input type=\"text\" placeholder=\"Enter town/city\"\n                                    value={this.state.search}\n                                    onChange={this.updateSearch.bind(this)}\n                                />\n                            </FormGroup>   \n                        </Form>\n                    </div>\n                </div>\n                <div className=\"row\">\n                    {filteredItems.map(item => {\n                        return <div className=\"col-12 col-md-3 mb-2\"><RenderDirectoryItem item={item} key={item.id} /></div>\n                    })}    \n                </div>\n            </div>\n        );\n    }\n}\n\nexport default GearDirectory;","/Users/alasdairselmes/Desktop/NucampFolder/3-React/themusicexchange/src/components/GearInfoComponent.js",["78","79"],"import React, { Component } from 'react';\nimport { Media, ButtonGroup, Button, Modal, ModalHeader, ModalBody, Form, FormGroup, Label, Input, FormFeedback } from 'reactstrap';\nimport { Link } from 'react-router-dom';\n\n\nfunction RenderGearItem({item}) {\n    return (\n        <div className=\"col my-5\">\n            \n            <Media>\n            <div className=\"row\">\n                <div className=\"col-12 col-md-4 mb-4\">\n                    <Media left >\n                        <Media object width=\"100%\" src={item.image} alt={item.name} />\n                    </Media>\n                </div>\n                <div className=\"col-12 col-md-8\">\n                    <Media body>\n                        <Media heading id=\"item-heading\">\n                            {item.name}\n                        </Media>\n                        <p>$ {item.price}</p>\n                        {item.trade ? <p className=\"text-success\">Trades Accepted</p> : <p className=\"text-danger\">No Trades</p> }\n                        <p className=\"text-muted\">Category: {item.category}</p>\n                        <p className=\"text-muted\">Location: {item.location}</p>\n                        <p>{item.description}</p>\n                    </Media>\n                </div>\n            </div>\n            </Media>\n        </div>\n    );\n}\n\nclass GearItemInfo extends Component {\n\n    constructor(props) {\n        super(props);\n\n        this.toggleModal = this.toggleModal.bind(this);\n        this.handleInputChange = this.handleInputChange.bind(this);\n        this.handleSubmit = this.handleSubmit.bind(this);\n\n        this.state = {\n            isModalOpen: false,\n            yourname: '',\n            youremail: '',\n            yourmessage: '',\n            touched: {\n                yourname: false,\n                youremail: false,\n                yourmessage: false\n            },\n            isSubmitted: false\n        };\n\n    }\n\n    validate(yourname, youremail, yourmessage) {\n\n        const errors = {\n            yourname: '',\n            youremail: '',\n            yourmessage: ''\n        };\n\n        if (this.state.touched.yourname) {\n            if (yourname.length < 2) {\n                errors.yourname = 'Your name must be at least 2 characters.';\n            } else if (yourname.length > 25) {\n                errors.yourname = 'Your name must be 25 or less characters.';\n            }\n        }\n\n        if (this.state.touched.youremail && !youremail.includes('@')) {\n            errors.youremail = 'Email should contain a @';\n        }\n\n        if (this.state.touched.yourmessage) {\n            if (yourmessage.length < 20) {\n                errors.yourmessage = 'Message must be at least 20 characters.';\n            } else if (yourmessage.length > 500) {\n                errors.yourmessage = 'Message must be 500 or less characters.';\n            }\n        }\n\n        return errors;\n\n    }\n\n    handleBlur = (field) => () => {\n        this.setState({\n            touched: {...this.state.touched, [field]: true}\n        });\n    }\n\n    handleInputChange(event) {\n        const target = event.target;\n        const name = target.name;\n        const value = target.type === 'checkbox' ? target.checked : target.value;\n\n        this.setState({\n            [name]: value\n        });\n    }\n\n    handleSubmit(event) {\n        console.log(\"Current state is: \" + JSON.stringify(this.state));\n        {/*alert(\"Current state is: \" + JSON.stringify(this.state));*/}\n        this.setState({\n            isSubmitted: true\n        });\n    }\n\n    toggleModal() {\n        this.setState({\n            isModalOpen: !this.state.isModalOpen\n        });\n    }\n\n    render() {\n\n        const errors = this.validate(this.state.yourname, this.state.youremail, this.state.yourmessage);\n\n        return (\n            <React.Fragment>\n                <div className=\"container\">\n                    {\n                    this.props.item\n                    ?\n                    <div className=\"row\">\n                        <RenderGearItem item={this.props.item} />\n                    </div>  \n                    :\n                    <div></div>  \n                    }\n                    <div className=\"row\">\n                        <div className=\"col\">\n                            <ButtonGroup size=\"lg\">\n                                <Link to=\"/\">\n                                    <Button id=\"buy-btn\"><i class=\"fa fa-dollar\"></i> Buy</Button>\n                                </Link>\n                                <Link onClick={this.toggleModal}>\n                                    <Button id=\"message-seller-btn\" className=\"mx-2\"><i class=\"fa fa-envelope\" ></i> Message Seller</Button>\n                                </Link>\n                                {this.props.item.trade ? \n                                    <Link to=\"/\">\n                                        <Button id=\"trade-btn\"><i class=\"fa fa-exchange\"></i> Trade</Button>\n                                    </Link>\n                                :\n                                <div></div>\n                                }\n                            </ButtonGroup>\n                        </div>\n                    </div>\n                    <div className=\"row\">\n                        <div className=\"col my-5\">\n                            <Link to=\"/geardirectory\">\n                                <Button><i class=\"fa fa-chevron-left\"></i> Back</Button>\n                            </Link>\n                        </div>\n                    </div>\n                    <div className=\"row\">\n                        <div className=\"col mb-5\">\n                            <h4>Have questions or comments? <Link to=\"/contactus\" className=\"text-info\">Contact us!</Link></h4>\n                        </div>\n                    </div>\n                </div>\n\n                <Modal isOpen={this.state.isModalOpen} toggle={this.toggleModal}>\n                        <ModalHeader toggle={this.toggleModal}>Message Seller</ModalHeader>\n                        <ModalBody>\n                            <Form onSubmit={this.handleSubmit}>\n                                <FormGroup >\n                                    <Label htmlFor=\"yourname\">Your Name</Label>   \n                                    <Input type=\"text\" id=\"yourname\" name=\"yourname\"\n                                        placeholder=\"Your Name\"\n                                        value={this.state.yourname}\n                                        invalid={errors.yourname}\n                                        onBlur={this.handleBlur(\"yourname\")}\n                                        onChange={this.handleInputChange} />\n                                    <FormFeedback>{errors.yourname}</FormFeedback>    \n                                </FormGroup>\n\n                                <FormGroup>\n                                    <Label htmlFor=\"youremail\">Your Email</Label>   \n                                        <Input type=\"text\" id=\"youremail\" name=\"youremail\"\n                                            placeholder=\"Your Email\"\n                                            value={this.state.youremail}\n                                            invalid={errors.youremail}\n                                            onBlur={this.handleBlur(\"youremail\")}\n                                            onChange={this.handleInputChange} />\n                                        <FormFeedback>{errors.youremail}</FormFeedback>  \n                                </FormGroup>\n\n                                <FormGroup>\n                                    <Label htmlFor=\"yourmessage\">Your Message</Label>\n                                    <Input type=\"textarea\" id=\"yourmessage\" name=\"yourmessage\"\n                                        rows=\"12\"\n                                        value={this.state.yourmessage}\n                                        onChange={this.handleInputChange}\n                                        value={this.state.yourmessage}\n                                        invalid={errors.yourmessage}\n                                        onBlur={this.handleBlur(\"yourmessage\")}\n                                        ></Input>\n                                    <FormFeedback>{errors.yourmessage}</FormFeedback> \n                                </FormGroup>\n\n                                <Button type=\"submit\" value=\"submit\" color=\"primary\">Send</Button>\n                            </Form>\n                            {this.state.isSubmitted ? <p className=\"mt-2 text-success\">Message Sent!</p> : <div></div>}\n                        </ModalBody>\n                </Modal>\n\n            </React.Fragment>\n        );\n    }\n}\n\nexport default GearItemInfo;","/Users/alasdairselmes/Desktop/NucampFolder/3-React/themusicexchange/src/components/AboutComponent.js",["80"],"import React from 'react';\nimport { Link } from 'react-router-dom';\n\nfunction About(props) {\n    return (\n        <div className=\"container\" id=\"about-component\">\n            <div className=\"row my-5\">\n                <div className=\"col\">\n                    <h1>Welcome to The Music Exchange!</h1>\n                    <h4>Connecting local musicians to gear, gigs, instructors, and each other since 2020...</h4>\n                    <hr />\n                </div>\n            </div>\n            <div className=\"row mb-5\">\n                <div className=\"col-12 col-md-5 mb-3 mb-md-0\">\n                    <img width=\"100%\" src=\"/assets/images/community.jpeg\"></img>\n                </div>\n                <div className=\"col-12 col-md-7\">\n                    <h3>Our Mission</h3>\n                    <p>We started The Music Exchange to help musicians find their community.</p>\n                    <p>We know how hard it can be to find like-minded musicians, instructors, gig opportunites, and quality used gear locally. </p>\n                    <p>Finding and searching through multiple websites, social media groups, or coffee shop notice boards is time-consuming, exhausting, and frustrating - especially if you are new to town.</p>\n                    <p>We aim to be a hub in your local community where musicians can find everything they need in one place!</p>\n                    <p><strong>- The Music Exchange Team</strong></p>\n                </div>\n            </div>\n            <div className=\"row\">\n                <div className=\"col mb-5\">\n                    <h4>Have questions or comments? <Link to=\"/contactus\" className=\"text-info\">Contact us!</Link></h4>\n                </div>\n            </div>\n        </div>\n    );\n\n}\n\nexport default About;","/Users/alasdairselmes/Desktop/NucampFolder/3-React/themusicexchange/src/components/RenderDirectoryItem.js",[],"/Users/alasdairselmes/Desktop/NucampFolder/3-React/themusicexchange/src/redux/configureStore.js",[],{"ruleId":"81","severity":1,"message":"82","line":17,"column":33,"nodeType":"83","endLine":17,"endColumn":83},{"ruleId":"84","replacedBy":"85"},{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","severity":1,"message":"89","line":3,"column":10,"nodeType":"90","messageId":"91","endLine":3,"endColumn":14},{"ruleId":"92","severity":1,"message":"93","line":87,"column":37,"nodeType":"83","endLine":95,"endColumn":39},{"ruleId":"92","severity":1,"message":"93","line":112,"column":37,"nodeType":"83","endLine":120,"endColumn":39},{"ruleId":"92","severity":1,"message":"93","line":137,"column":37,"nodeType":"83","endLine":146,"endColumn":39},{"ruleId":"92","severity":1,"message":"93","line":164,"column":37,"nodeType":"83","endLine":171,"endColumn":39},{"ruleId":"92","severity":1,"message":"94","line":188,"column":45,"nodeType":"83","endLine":192,"endColumn":47},{"ruleId":"92","severity":1,"message":"95","line":198,"column":37,"nodeType":"83","endLine":199,"endColumn":70},{"ruleId":"92","severity":1,"message":"96